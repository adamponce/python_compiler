link puny
procedure test_concatenate_strings()
input1:="Hello"
input2:="world"
expected_output:="Helloworld"
output:=""
every char := 1 to input1 do
{output+:=char
}
every char := 1 to input2 do
{output+:=char
}
if output=expected_output then
{print(="test_concatenate_strings PASSED")
}
else 
{print("test_concatenate_strings FAILED: ", output, " != ", expected_output)


}
end
procedure test_get_string_length()
input_str:="Hello, world!"
expected_output:=13
output:=0
every char := 1 to input_str do
{output+:=1
}
if output=expected_output then
{print(="test_get_string_length PASSED")
}
else 
{print("test_get_string_length FAILED: ", (output), " != ", (expected_output))


}
end
procedure run_tests()
test_concatenate_strings()
test_get_string_length()


end
if __name__=='__main__' then
{run_tests()

}
end
